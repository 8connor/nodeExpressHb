<div class="container">
    <div class="col-md-6">
        <input type="text" id="input">
        <button type="button" id="submitBtn">submit
        </button>
        <ol id="theList">
            {{#each burgers}}
            <li class="burgerText">
                {{this.burger_name}}
            </li>
            <button type="button" class="isDevoured" id="{{@index}}">devour</button>
            {{/each}}
        </ol>
    </div>
    <div class="col-md-6">
        <h1>Devoured Burgers</h1>
        <ol id="theDevoured">
            {{#each burgersDevoured}}
            <li>
                {{this}}
            </li>
            {{/each}}
        </ol>
    </div>
</div>




